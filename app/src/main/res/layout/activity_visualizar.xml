<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="UselessParent">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:focusable="true"
            android:layout_margin="5dp"
            android:shape="ring"
            android:thicknessRatio="2"
            app:cardCornerRadius="5dp">

            <TextView
                android:id="@+id/SetData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_margin="4dp"
                android:text="02/05/2018"
                android:textColor="@android:color/background_dark"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/SetLocal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:gravity="start"
                    android:text="Local: Lamego DSADASDASDASD"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:maxLines="1"
                    android:ellipsize="end"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/SetLocalidade"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:text="Localidade: IFMA"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:maxLines="1"
                    android:ellipsize="end"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_margin="10dp"
                    android:background="@android:color/background_dark">
                </View>

                <TextView
                    android:id="@+id/SetLatitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:text="Latitude: -40.4245"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:maxLines="1"
                    android:ellipsize="end"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/SetLongitude"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:text="Longitude: 104.1245"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:maxLines="1"
                    android:ellipsize="end"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_margin="10dp"
                    android:background="@android:color/background_dark">
                </View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ1"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 1: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ2"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 2: 3.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ3"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 3: 6.6"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ4"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 4: 7.7"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ5"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 5: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ6"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 6: 3.4"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ7"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 7: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ8"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 8: 3.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ9"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 9: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ10"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 10: 3.8"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="start">

                    <TextView
                        android:id="@+id/SetQ11"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 11: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <View
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="@color/colorPrimary"
                        android:layout_margin="10dp">
                    </View>

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/SetQ12"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:text="Pergunta 11: 2.0"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <TextView
                    android:id="@+id/SetScoreFinal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    android:text="Score Final: 0.512321"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_margin="10dp"
                    android:background="@android:color/background_dark">
                </View>

                <LinearLayout
                    android:id="@+id/ContainerImagens"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/SetImagens"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Imagens 1/4"
                        tools:ignore="HardcodedText" />

                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:fillViewport="false"
                        android:measureAllChildren="false"
                        android:scrollbars="none">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/SetImage1"
                                android:layout_width="130dp"
                                android:layout_height="110dp"
                                tools:ignore="ContentDescription" />

                            <ImageView
                                android:id="@+id/SetImage2"
                                android:layout_width="130dp"
                                android:layout_height="110dp"
                                tools:ignore="ContentDescription" />

                            <ImageView
                                android:id="@+id/SetImage3"
                                android:layout_width="130dp"
                                android:layout_height="110dp"
                                tools:ignore="ContentDescription" />

                            <ImageView
                                android:id="@+id/SetImage4"
                                android:layout_width="130dp"
                                android:layout_height="110dp"
                                tools:ignore="ContentDescription" />

                        </LinearLayout>

                    </HorizontalScrollView>

                </LinearLayout>

                <com.rey.material.widget.Button
                    android:id="@+id/SetExportar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:background="@drawable/round"
                    android:drawableStart="@drawable/share"
                    android:text="Compartilhar "
                    android:textAllCaps="false"
                    app:rd_enable="true"
                    app:rd_rippleType="wave"
                    tools:ignore="HardcodedText" />

            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>

</LinearLayout>
